{
  "name": "vue-auth-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "format": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^13.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,css,md,json,vue}": [
      "prettier --write"
    ]
  }
}
